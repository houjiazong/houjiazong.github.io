@import url('https://fonts.googleapis.com/css?family=Rubik:0,100,200,300,400,500,600,700,800,900')

body {
  font-family: 'Rubik', -apple-system, BlinkMacSystemFont, 'Segoe UI', oboto, Oxygen, Ubuntu, Cantarell, 'Open Sans', 'Helvetica Neue', sans-serif;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
  font-size: 16px;
  color: #121314;
}

#app {
  height: 100vh;
}

.navbar {
  background-color: rgba(255, 255, 255, .8);
  backdrop-filter: blur(4px);
  box-shadow: 0 0 8px 3px #eee;
}

.navbar .links
  background-color: transparent !important;
